{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.2",
  "body": [
    {
      "type": "TextBlock",
      "text": "${title}",
      "wrap": true
    },
    {
      "type": "Container",
      "$data": "${members}",
      "separator": true,
      "items": [
        {
          "type": "TextBlock",
          "text": "${displayName}",
          "wrap": true,
          "size": "large"
        },
        {
          "type": "TextBlock",
          "text": "${currentTime}",
          "wrap": false,
          "size": "small"
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Action.OpenUrl",
      "title": "Schedule A Meeting",
      "url": "${url}"
    }
  ]
}