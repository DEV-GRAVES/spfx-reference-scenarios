  "requiresCustomScript": false,
  "supportedHosts": ["Dashboard", "SharePointWebPart"],

  "preconfiguredEntries": [{
    "groupId": "bd067b1e-3ad5-4d5d-a5fe-505f07d7f59c", // Dashboard
    "group": {
      "default": "Dashboard"
    },
    "title": {
      "default": "Company News"
    },
    "description": {
      "default": "Company News Adaptive Card Extension Sample"
    },
    "officeFabricIconFontName": "News",
    "properties": {
      "title": "Company News",
      "description": "Company News Adaptive Card Extension Sample",
      "iconProperty": "News" // Default to sharepointlogo
    }
  }]
}