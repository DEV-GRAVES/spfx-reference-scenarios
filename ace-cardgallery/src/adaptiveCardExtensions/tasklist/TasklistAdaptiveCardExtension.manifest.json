  "requiresCustomScript": false,
  "supportedHosts": ["Dashboard", "SharePointWebPart"],

  "preconfiguredEntries": [{
    "groupId": "bd067b1e-3ad5-4d5d-a5fe-505f07d7f59c", // Dashboard
    "group": {
      "default": "Dashboard"
    },
    "title": {
      "default": "My Tasks"
    },
    "description": {
      "default": "Task List Adaptive Card Extension Sample"
    },
    "officeFabricIconFontName": "TaskSolid",
    "properties": {
      "title": "My Tasks",
      "description": "Review your tasks",
      "iconProperty": "TaskSolid" // Default to sharepointlogo
    }
  }]
}